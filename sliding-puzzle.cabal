category:            Game
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable simple-solver
  main-is:             SimpleSolver.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.6 && <4.7, vector >=0.10 && <0.11, containers >=0.5 && <0.6, mtl >=2.1 && <2.2
  -- hs-source-dirs:      
  default-language:    Haskell2010
  ghc-options:         -O2 -Wall

executable gif-solver
  main-is:             GifSolver.hs
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.6 && <4.7, vector >=0.10 && <0.11, containers >=0.5 && <0.6, mtl >=2.1 && <2.2, split >= 0.2.2 && <0.3, diagrams-rasterific >= 0.1 && <0.2, diagrams-lib >= 1.2 && <1.3
  -- hs-source-dirs:      
  default-language:    Haskell2010
  ghc-options:         -O2 -Wall
